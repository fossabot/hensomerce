{
  "name": "Hensomerce",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Homescreen - New tab extension. Made with extensionizr.com",
  "homepage_url": "http://github.com/treipatru",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "bookmarks",
    "history",
    "management"
  ],
  "optional_permissions": [
    "webRequest"
  ]
}